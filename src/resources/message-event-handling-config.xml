<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean id="message" class="net.proselyte.autowiring.ex6.Message">
        <property name="message" value="11111111111"/>
    </bean>
    <bean id="myStartEventHandler"
          class="net.proselyte.autowiring.ex6.MyStartEventHander"/>
    <bean id="myStopEventHandler"
          class="net.proselyte.autowiring.ex6.MyStopEventHandler"/>
</beans>